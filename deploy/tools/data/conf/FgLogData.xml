<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
 <logging>
  <log>
   <filename>path/to/log/own_aircraft_log.txt</filename>
   <enabled>true</enabled>
   <interval-ms>10</interval-ms>
   <delimiter>,</delimiter>
	<entry>
    		<enabled>true</enabled>
    		<title>Time[GMT]</title>
    		<property>/sim/time/gmt-string</property>
	</entry>   	
	<entry>
    		<enabled>true</enabled>
    		<title>Latitude[Deg]</title>
    		<property>/position/latitude-deg</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>Longitude[Deg]</title>
    		<property>/position/longitude-deg</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>Altitude[Ft]</title>
    		<property>/position/altitude-ft</property>
	</entry>	
	<entry>
    		<enabled>true</enabled>
    		<title>Roll[Deg]</title>
    		<property>/orientation/roll-deg</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>Pitch[Deg]</title>
    		<property>/orientation/pitch-deg</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>Yaw[Deg]</title>
    		<property>/orientation/heading-deg</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>RollRate[Deg/s]</title>
    		<property>/orientation/roll-rate-degps</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>PitchRate[Deg/s]</title>
    		<property>/orientation/pitch-rate-degps</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>YawRate[Deg/s]</title>
    		<property>/orientation/yaw-rate-degps</property>
	</entry>	
	<entry>
    		<enabled>true</enabled>
    		<title>TrueAirSpeed[kt]</title>
    		<property>/velocities/airspeed-kt</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>GroundSpeed[kt]</title>
    		<property>/velocities/groundspeed-kt</property>
	</entry>
	<entry>
    		<enabled>true</enabled>
    		<title>RadioAltitude[m]</title>
    		<property>/position/altitude-ft</property>
	</entry>	
	<entry>
    		<enabled>true</enabled>
    		<title>BaroAltitude[m]</title>
    		<property>/position/altitude-ft</property>
	</entry>			
  </log>
 </logging>
</PropertyList>